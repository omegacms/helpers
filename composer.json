{
  "name": "omegacms/helpers",
  "type": "package",
  "description": "Omega CMS Logging Package",
  "keywords": [ "omegacms", "framework", "helpers"],
  "homepage": "https://omegacms.github.io",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Adriano Giovannini",
      "email": "agisoftt@gmail.com",
      "homepage": "https://omegacms.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "omegacms/application": "^1.0.0"
  },
  "require-dev": {
    "omegacms/testing": "^1.0.0",
    "phpunit/phpunit": "^9.5",
    "symfony/process": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Omega\\Helpers\\": "src/"
    },
    "files": [
      "src/function.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Omega\\Helpers\\Tests\\": "tests/"
    },
    "files": [
      "src/function.php"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable"
}
